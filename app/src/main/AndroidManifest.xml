<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.strath.countyandgovernor_registrationsystem">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CountyandGovernor_RegistrationSystem">
        <activity
            android:name=".views.Reports"
            android:exported="false" />
        <activity android:name=".recview.EdFarm" />
        <activity android:name=".views.Sign" />
        <activity android:name=".views.Login" />
        <activity android:name=".dashboard.Dashboard" />
        <activity android:name=".recview.countyRecView" />
        <activity android:name=".recview.governorRecView" />
        <activity android:name=".views.GovernorForm" />
        <activity android:name=".views.CountyForm"/>
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>